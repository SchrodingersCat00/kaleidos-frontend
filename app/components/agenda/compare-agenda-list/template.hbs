
  <div class="vl-grid vl-grid--is-stacked vl-u-spacer-extended-bottom-l">
    <div class="compare-left-column">
      {{utils/agenda-selector
        searchField="name"
        propertyToShow="name"
        placeholder="Selecteer een agenda"
        multiSelect=false
        sortField="label"
        selectedItems=agendaOne
        selectModel=(action "chooseAgendaOne")
      }}
    </div>
    <div class="compare-right-column">
      {{utils/agenda-selector
        searchField="name"
        propertyToShow="name"
        placeholder="Selecteer een agenda"
        multiSelect=false
        sortField="label"
        selectedItems=agendaTwo
        selectModel=(action "chooseAgendaTwo")
      }}
    </div>
  </div>

  {{#if hasChangedSet}}

  {{#each changedGroups as |changedSet|}}
    {{agenda/compare-agenda-item changedSet=changedSet}}
  {{/each}}

  {{else}}

    <div class="vl-u-spacer-extended-top-s">
      {{web-components/vl-alert message=(t "no-agendaitems-yet")}}
    </div>

  {{/if}}
