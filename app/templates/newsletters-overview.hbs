{{#if (await model)}}
  {{newsletter/newsletter-header-overview
    agenda=(await model.agenda)
    amountOfItems=(await model.amountOfItems)
  }}
  <div class="vlc-scroll-wrapper__body">
    <div class="vl-u-spacer-extended-l">
      <div class="vlc-container">
        {{newsletter/newsletter-meeting meeting=(await model.meeting)}}
        {{#each (await model.agendaitemGroups) as |group|}}
          {{#each group.groups as |agendaitemGroup|}}
            {{#each agendaitemGroup.agendaitems as |agendaitem|}}
              {{newsletter/newsletter-item
                agenda=(await model.agenda)
                index=agendaitem.priority
                agendaitem=agendaitem
              }}
            {{/each}}
          {{/each}}
        {{/each}}
      </div>
      {{!-- {{#if (not (eq (await model.agendaitems.length) 0))}}
        {{web-components/vl-pagination
          page=page
          size=size
          links=(await links)
          nbOfItems=nbOfItems
          total=total
        }}
      {{/if}} --}}
    </div>
  </div>
{{/if}}